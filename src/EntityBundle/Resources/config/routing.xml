    <default key="_controller">EntityBundle:Categorie:search</default>
  </route>
  <route id="static_create" path="/static/create">
    <default key="_controller">EntityBundle:Static:create</default>
  </route>
  <route id="static_edit" path="/static/edit">
    <default key="_controller">EntityBundle:Static:edit</default>
  </route>
  <route id="static_delete" path="/static/delete">
