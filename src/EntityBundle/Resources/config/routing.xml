<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">
  <route id="entity_homepage" path="/categorie">
    <default key="_controller">EntityBundle:Default:index</default>
  </route>
  <route id="categorie_all" path="/categorie/all">
    <default key="_controller">EntityBundle:Categorie:all</default>
  </route>
  <route id="categorie_view" path="/categorie/view">
    <default key="_controller">EntityBundle:Categorie:view</default>
  </route>
