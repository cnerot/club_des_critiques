  </route>
  <route id="categorie_remove" path="/categorie/remove">
    <default key="_controller">EntityBundle:Categorie:remove</default>
  </route>
  <route id="attribute_all" path="/attribute/all">
    <default key="_controller">EntityBundle:Attribute:all</default>
  </route>
  <route id="attribute_create" path="/attribute/create">
    <default key="_controller">EntityBundle:Attribute:create</default>
  </route>
  <route id="attribute_view" path="/attribute/view">
    <default key="_controller">EntityBundle:Attribute:view</default>
