<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="EntityBundle\Repository\valuedateRepository" name="EntityBundle\Entity\valuedate">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="idAttribute" type="integer" column="id_attribute"/>
    <field name="idEntity" type="integer" column="id_entity"/>
    <field name="value" type="datetime" column="value"/>
  </entity>
</doctrine-mapping>
